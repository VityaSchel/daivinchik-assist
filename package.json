{
  "name": "daivinchik-assist",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:standalone": "yarn expo-electron start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "sudo eas build --platform android --local",
    "build:standalone:dev": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null -c.compression=store",
    "build:standalone:prod": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.19",
    "node": ">= 16.13.0"
  },
  "dependencies": {
    "electron": "^6.0.12",
    "expo": "~46.0.13",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.17.2",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "typescript": "~4.3.5"
  },
  "private": true
}
